* {
	box-sizing: border-box;
}
body {
    margin: 0px;
    padding: 0px;
}
#mainBody{
	overflow:hidden;
}
a {
	text-decoration: none;
	color: black;
}

@font-face {
    font-family: 'SamsungOneGui-400';
    src: url("../resources/font/SamsungOneGui/SamsungOneUI-400.woff") format('woff'),
	url("../resources/font/SamsungOneGui/SamsungOneUI-400.woff") format('woff');
}
@font-face {
    font-family: 'SamsungOneGui700';
    src: url("../resources/font/SamsungOneGui/SamsungOneUI-700.woff") format('woff'), 
	url("../resources/font/SamsungOneGui/SamsungOneUI-700.woff") format('woff');
}
@font-face {
    font-family: 'SamsungOneGui-700';
    font-style:  normal;
    src: url("../resources/font/SamsungOneGui/SamsungOneUI-700.woff"), url("../resources/font/SamsungOneGui/SamsungOneUI-700.woff");
}
@font-face {
    font-family: 'SamsungOneGui400';
    font-style:  normal;
    src: url("../resources/font/SamsungOneGui/SamsungOneUI-400.woff"), url("../resources/font/SamsungOneGui/SamsungOneUI-400.woff");
}
@font-face {
    font-family: 'SamsungOneGui-400';
    font-style:  normal;
    src: url("../resources/font/SamsungOneGui/SamsungOneUI-400.woff"), url("../resources/font/SamsungOneGui/SamsungOneUI-400.woff");
}

.contianer {
    width: 100%;
    margin: 0px auto;
    position: relative;
}
header {
    width: 100%;
    height: 84px;
    background-color: #00bcd5;
    position: fixed;
    margin: 0px auto;
    z-index: 2;
    
    background-image: url("../resources/images/logo.jpg");
    background-repeat: no-repeat;
    background-position:  center center;
}
header .topSearch {
    position: absolute;
    top: 23px;
    right: 26px;
    width: 288px;
    height: 38px;
    background-color: green;
    background: url("../resources/images/searchbg.png");
    color: #a9e5f0;
    line-height: 38px;
    padding-left: 18px;
    font-family: SamsungOneGui-400;
    font-size: 16px;
}

header .topIcon {
    position: absolute;
    top: 206px;
    right: 2px;
    width: 46px;
    height: 32px;
    background-color: green;
    background: url("../resources/images/btn_gototop.png");
	cursor: pointer;
}

nav {
    width: 240px;
    height: calc(100%);
    background-color: #f6f6f6;
    border-right: 1px solid #d8e2e3;
    position: fixed;
    margin: 84px auto 0px auto;
    z-index: 1;
}

nav ul {
    list-style: none;
    margin: 26px 0px 0px 0px;
    padding: 0;
}
nav ul li {
    color: black;
    font-family: SamsungOneGui-400;
    font-size: 16px;
    padding: 12px 0px 12px 25px;
    text-transform: uppercase;
	overflow:hidden;
}
nav ul li span{
	z-index:10;
}
nav ul li.focus {
	font-family: SamsungOneGui-700;
}

nav ul li ul {
    list-style: none;
    margin: 11px 0px 0px 0px;
    padding: 0;
}
nav ul li ul li {
    color: black;
    font-family: SamsungOneGui-400;
    font-size: 16px;
    padding: 8px 0px 8px 14px;
    text-transform: capitalize;
}
nav ul li ul li.focus {
	font-family: SamsungOneGui-700;
	color: #f7364d;
}
.subMenuCont{
	overflow:hidden;
	height:auto;
	width:inherit;
}

section.rightPanel {
    width: calc(100% - 240px);
    position: relative;
    left: 240px;
    top: 84px;
}

section .heading {
    height: 107px;
	border-bottom: 1px solid #e0e0e0;
	position: fixed;
	
	top:0px;
}
section .heading .title {
    color: #f87885;
    font-family: SamsungOneGui-400;
    font-size: 50px;
	line-height: 107px;
    padding: 0px 0px 0px 60px;
    text-transform: capitalize;
}

section .heading .menu {
	position: absolute;
	width: 720px;
	height: 50px;
	right: 0px;
	bottom: 0px;
}
section .heading .menu ul {
	list-style: none;
	display: inline;
	margin: 0;
	padding: 0;
	float: right;
}
section .heading .menu ul li {
	display: inline;
	height: 50px;
	line-height: 50px;
	text-align: right;
	margin: 0px 50px 0px 0px;
    color: black;
    font-family: SamsungOneGui-400;
    font-size: 20px;
}
section .heading .menu ul li::after {
	content: "|";
	position: relative;
	left: 25px;
    color: black;
    font-family: SamsungOneGui-400;
    font-size: 20px;
}

section.subContainer article.para {
	margin: 60px 60px 60px 58px;
}
section.subContainer article.para .title {
	height: 48px;
